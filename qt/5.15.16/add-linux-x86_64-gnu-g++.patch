diff -Naur src/qtbase/mkspecs/linux-x86_64-gnu-g++/qmake.conf src_fix/qtbase/mkspecs/linux-x86_64-gnu-g++/qmake.conf
--- src/qtbase/mkspecs/linux-x86_64-gnu-g++/qmake.conf	1970-01-01 08:00:00.000000000 +0800
+++ src_fix/qtbase/mkspecs/linux-x86_64-gnu-g++/qmake.conf	2025-07-21 22:53:58.098050688 +0800
@@ -0,0 +1,36 @@
+#
+# qmake configuration for building with x86_64-linux-gnu-g++
+#
+
+MAKEFILE_GENERATOR      = UNIX
+CONFIG                 += incremental
+QMAKE_INCREMENTAL_STYLE = sublib
+
+include(../common/linux.conf)
+include(../common/gcc-base-unix.conf)
+include(../common/g++-unix.conf)
+
+QMAKE_INCDIR_OPENGL     = $(OPENGL_INC_DIR)
+QMAKE_LIBDIR_OPENGL     = $(OPENGL_LIB_DIR)
+
+QMAKE_INCDIR_OPENGL_ES2 = $(OPENGL_INC_DIR)
+QMAKE_LIBDIR_OPENGL_ES2 = $(OPENGL_LIB_DIR)
+QMAKE_INCDIR_EGL        = $(OPENGL_INC_DIR)
+QMAKE_LIBDIR_EGL        = $(OPENGL_LIB_DIR)
+QMAKE_LIBS_OPENGL_ES2   = $(OPENGL_LIB_DIR)
+QMAKE_LIBS_EGL          = $(OPENGL_LIB_DIR)
+
+QMAKE_LFLAGS            += -Wl,--rpath-link,$$QMAKE_LIBDIR_OPENGL
+
+# modifications to g++.conf
+QMAKE_CC                = $(CROSSTOOL_PREFIX)gcc
+QMAKE_CXX               = $(CROSSTOOL_PREFIX)g++
+QMAKE_LINK              = $(CROSSTOOL_PREFIX)g++
+QMAKE_LINK_SHLIB        = $(CROSSTOOL_PREFIX)g++
+
+# modifications to linux.conf
+QMAKE_AR                = $(CROSSTOOL_PREFIX)ar cqs
+QMAKE_OBJCOPY           = $(CROSSTOOL_PREFIX)objcopy
+QMAKE_NM                = $(CROSSTOOL_PREFIX)nm -P
+QMAKE_STRIP             = $(CROSSTOOL_PREFIX)strip
+load(qt_config)
\ No newline at end of file
